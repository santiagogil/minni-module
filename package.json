{
  "name": "minni-module",
  "version": "0.0.0-development",
  "description": "Minimal npm module scaffolding",
  "main": "index.js",
  "preferGlobal": true,
  "bin": "./minni-module.js",
  "scripts": {
    "test": "standard && nsp check && istanbul cover tape test.js",
    "lint-prose": "write-good $(bash -c 'echo {*,templates/*}.md')",
    "commit": "git-cz",
    "setup": "npm i && semantic-release-cli setup",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/santiagogil/minni-module.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/santiagogil/minni-module/issues"
  },
  "homepage": "https://github.com/santiagogil/minni-module#readme",
  "devDependencies": {
    "commitizen": "*",
    "cz-conventional-changelog": "*",
    "istanbul": "^0.4.5",
    "nsp": "^3.1.0",
    "semantic-release": "^8.2.0",
    "semantic-release-cli": "*",
    "standard": "^10.0.3",
    "tape": "^4.8.0",
    "write-good": "^0.11.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "conf": "^1.3.1",
    "glob": "^7.1.2",
    "minimist": "^1.2.0",
    "mustache": "^2.3.0",
    "rimraf": "^2.6.2"
  }
}
